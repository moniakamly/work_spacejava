package projet1;

public class Ville {
	
	//retourne le nom de la ville (accesseur=getter)
	public String getNom()  {
		return nomVille;
	}
	
	//retourne nom du Pays
	public String getNomPays( ) {
		return nomPays;
	}
	
	
	private String nomVille;
	private String nomPays;
	private int nbrHabitants;
	
	
public Ville ( ) {
	nomVille = "Gafsa";
	nomPays = "Tunisie";
	nbrHabitants = 250000; 
}
}
