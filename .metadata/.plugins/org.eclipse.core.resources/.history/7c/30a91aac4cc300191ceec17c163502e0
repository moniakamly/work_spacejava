package ClassesandObjects;

public class App {
	
	public static void main(String[] args) {
		Book myFirstBook = new Book();
		myFirstBook.bookTitle = "The Davinci Code";
		myFirstBook.author = "Dan Brown";
		myFirstBook.numberOfPages = 354;
		myFirstBook.genre= "Thriller";
		
		
		Book mySecondBook = new Book();
		mySecondBook.bookTitle = "The Book thief";
		mySecondBook.author = "Marcus Zusac";
		mySecondBook.numberOfPages = 236;
		mySecondBook.genre = "Fiction";
		
		System.out.println(mySecondBook.genre);

		 
	}
	
		
	
}
